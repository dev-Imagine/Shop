@{
    ViewBag.Title = "LogIn";
}
<header id="fh5co-header" class="fh5co-cover fh5co-cover-sm" role="banner" style="background-image:url(../../Content/images/img_bg_2.jpg);">
    <div class="overlay"></div>
    <div class="container">
        <div class="display-t text-center">
            <div class="display-tc animate-box" data-animate-effect="fadeIn">
                <h1>Bienvenido</h1>
                <h2>Inicia sesion</h2>
            </div>
        </div>
    </div>
</header>

<div id="fh5co-contact">
    <div class="container">
        <div class="row">
            <div class="col-md-12 animate-box" style="text-align:center">
                <form method="post" action="@Url.Action("Ingresar", "Admin")" autocomplete="off">
                    <div class="row form-group">
                        <div class="col-md-6" style="padding-bottom:5px;">
                            <input type="text" name="oUser.nombreUsuario" id="nombreUsuario" class="form-control" placeholder="Usuario">
                        </div>
                        <div class="col-md-6">
                            <input type="password" name="oUser.contraseña" id="passwordUsuario" class="form-control" placeholder="password">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-12 col-md-6 offset-md-3">
                            <button type="submit" class="btn btn-primary w-100">INGRESAR</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>

    </div>
</div>
